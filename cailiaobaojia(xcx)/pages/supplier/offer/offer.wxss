page{background:var(--main-color)}.supplier-offer{padding:32rpx}.supplier-offer .base{background:#fff;border-radius:10rpx;margin-bottom:32rpx}.supplier-offer .offer-header{padding:0 32rpx}.supplier-offer .offer-header .cell-title{min-width:180rpx}.supplier-offer .offer-table{font-size:26rpx;text-align:center}.supplier-offer .offer-table .common{display:flex;justify-content:space-between;flex:1;padding:0 20rpx;align-items:center}.supplier-offer .offer-table .common view{margin:0 10rpx}.supplier-offer .offer-table .common view input{width:90%}.supplier-offer .offer-table .common view:nth-child(1){width:25%}.supplier-offer .offer-table .common view:nth-child(2){width:12.5%}.supplier-offer .offer-table .common view:nth-child(3){width:20%}.supplier-offer .offer-table .common view:nth-child(4){width:20%}.supplier-offer .offer-table .common view:nth-child(5){width:22.5%}.supplier-offer .offer-table .table-header{color:var(--main-color);line-height:100rpx}.supplier-offer .offer-table .table-list{border-top:1px solid #CECECE;color:#666666;font-size:24rpx;padding:30rpx 10rpx}.supplier-offer .offer-upload{padding:32rpx;display:flex;align-items:center;font-size:24rpx;color:#EC4B4B}.supplier-offer .offer-upload .upload-title{font-size:30rpx;color:#333;min-width:245rpx}.supplier-offer .var-button{background:#fff;color:var(--main-color);margin-top:56rpx}.move-x{width:880rpx}.picker{font-size:28rpx;font-weight:400;color:#999999}.color-black{color:black !important}